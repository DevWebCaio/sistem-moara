# Configurações de Produção
NODE_ENV=production
NEXT_PUBLIC_APP_NAME=Solar DG Platform
NEXT_PUBLIC_APP_URL=https://sistem-moara.vercel.app

# Demo Mode (desativado para produção)
NEXT_PUBLIC_DEMO_MODE=false

# Stripe Configuration (substitua pelas suas credenciais reais)
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# SOLARMAN Configuration (substitua pelas suas credenciais reais)
SOLARMAN_API_URL=https://api.solarmanpv.com
SOLARMAN_APP_ID=your_solarman_app_id
SOLARMAN_APP_SECRET=your_solarman_app_secret
SOLARMAN_USERNAME=your_solarman_username
SOLARMAN_PASSWORD=your_solarman_password

# Email Configuration (substitua pelas suas credenciais reais)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# Supabase Configuration (se usar)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key 